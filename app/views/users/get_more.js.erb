<% if @category=='music' %>
	<% @songs.each do |song| %>
		$("#user_music").append("<%= j(render partial: 'song',locals: {song: song})%>");
	<% end %>

	<% if !@hasMore %>
		$("#get_music").remove();
	<% end %>	
<% elsif @category=='movies' %>
	<% @movies.each do |movie| %>
		$("#user_movies").append("<%= j(render partial: 'movie',locals: {movie: movie})%>");
	<% end %>

	<% if !@hasMore %>
		$("#get_movies").remove();
	<% end %>
<% elsif @category=='games' %>
	<% @games.each do |game| %>
		$("#user_games").append("<%= j(render partial: 'game',locals: {game: game})%>");
	<% end %>

	<% if !@hasMore %>
		$("#get_games").remove();
	<% end %>
<% elsif @category=='documents' %>
	<% @documents.each do |document| %>
		$("#user_documents").append("<%= j(render partial: 'document',locals: {document: document})%>");
	<% end %>

	<% if !@hasMore %>
		$("#get_documents").remove();
	<% end %>
<% else %>
<% end %>
$("[data-toggle='popover']").popover({trigger: "hover"});